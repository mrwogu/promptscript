{
  "name": "@promptscript/browser-compiler",
  "version": "0.0.1",
  "description": "Browser-compatible PromptScript compiler with in-memory file system",
  "keywords": [
    "browser",
    "compiler",
    "promptscript",
    "web",
    "playground"
  ],
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.d.ts"
    }
  },
  "dependencies": {
    "@promptscript/core": "workspace:^",
    "@promptscript/parser": "workspace:^",
    "@promptscript/validator": "workspace:^",
    "@promptscript/formatters": "workspace:^",
    "path-browserify": "^1.0.1"
  }
}
