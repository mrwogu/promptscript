{
  "name": "@promptscript/source",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://getpromptscript.dev/",
  "scripts": {
    "build": "nx run-many -t build",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "typecheck": "pnpm nx run-many -t typecheck",
    "schema:generate": "ts-node --esm scripts/generate-schema.mts",
    "schema:check": "./scripts/check-schema.sh",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build --strict",
    "docs:generate": "typedoc",
    "prs": "node --import @swc-node/register/esm-register packages/cli/src/cli.ts",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nx/esbuild": "^22.4.2",
    "@nx/eslint": "^22.4.2",
    "@nx/eslint-plugin": "^22.4.2",
    "@nx/js": "22.4.2",
    "@nx/vite": "^22.4.2",
    "@nx/vitest": "22.4.2",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.10",
    "@swc/core": "~1.15.10",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "22.4.2",
    "prettier": "~3.8.1",
    "ts-json-schema-generator": "^2.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "~5.9.2",
    "verdaccio": "^6.0.5",
    "vite": "^7.0.0",
    "vitest": "^4.0.18"
  },
  "nx": {
    "includedScripts": []
  }
}
